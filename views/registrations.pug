
block content
	table.table.table-striped.table-dark
		thead.thead-dark
			tr
				th First Name
				th Last Name
				th Address 1
				th Address 2
				th City
				th State
				th Zip
				th Country

		tbody
			for user in users
				tr
					th #{user.FirstName}


